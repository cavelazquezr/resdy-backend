  "license": "ISC",
  "keywords": [],
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "tsoa": "^5.1.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@types/geojson": "^7946.0.10",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
