{
	"name": "resdy-back",
	"version": "1.0.0",
	"description": "Rest API with Nodejs and Prisma",
	"main": "src/index.ts",
	"scripts": {
		"build": "tsc",
		"tsoa": "tsoa spec-and-routes",
		"prestart": "npm run build",
		"dev": "concurrently \"npx nodemon\" \"npx nodemon -x tsoa spec-and-routes\"",
		"start": "docker-compose -f docker-compose.yml up -d --renew-anon-volumes",
		"stop": "docker-compose -f docker-compose.yml down",
		"migrate": "npx prisma migrate dev",
		"seed": "ts-node src/seed/index.ts"
	},
	"author": "Carlos Vel√°zquez",
	"license": "ISC",
	"keywords": [],
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^20.3.1",
		"@types/ws": "^8.5.10",
		"concurrently": "^8.2.0",
		"nodemon": "^3.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-prisma": "^5.0.0",
		"prisma": "^5.7.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"tsoa": "^5.1.1",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"@neondatabase/serverless": "^0.7.2",
		"@prisma/adapter-neon": "^5.9.1",
		"@prisma/client": "^5.9.1",
		"@types/geojson": "^7946.0.10",
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"csv-parser": "^3.0.0",
		"dotenv-cli": "^7.3.0",
		"express": "^4.18.2",
		"express-openapi-validator": "^5.0.4",
		"jsonwebtoken": "^9.0.0",
		"swagger-ui-express": "^5.0.0",
		"ws": "^8.16.0"
	}
}
